<?xml version="1.0"?>
<package format="2">
  <name>testbench_sensor_status</name>
  <version>0.0.0</version>
  <description>roboy motor status plugin for rqt</description>
  <maintainer email="imbery@in.tum.de">Julius Imbery</maintainer>
  <license>BSD-3</license>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>roscpp</depend>
  <depend>roslint</depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>std_msgs</depend>
